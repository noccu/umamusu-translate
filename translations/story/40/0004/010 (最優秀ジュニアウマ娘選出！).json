        },
        {
            "jpName": "ハッピーミーク",
            "enName": "",
            "jpText": "ん。かんぱーい。",
            "enText": "Hm. KANPAI.",
            "nextBlock": 15,
            "origClipLength": 63,
            "animData": [
                {
                    "origLen": 73,
                    "pathId": -9662989104463525
                },
