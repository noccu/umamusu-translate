            "pathId": 8839780080875534735,
            "blockIdx": 4
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "来る日も――",
            "enText": "And the next――",
            "nextBlock": 6,
            "origClipLength": 52,
            "voiceIdx": -1,
            "animData": [
                {
                    "origLen": 25,
