            ],
            "pathId": 8157503238414940675,
            "blockIdx": 27
        },
        {
            "jpName": "エスポワールシチー",
            "enName": "Espoir City",
            "jpText": "……そうかよ。",
            "enText": "It is, huh?",
            "nextBlock": 29,
            "origClipLength": 96,
            "voiceIdx": 19,
            "animData": [
                {
                    "origLen": 141,
                    "pathId": 5114985783454109692
