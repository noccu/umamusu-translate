            "pathId": -8426117218018397356,
            "blockIdx": 13
        },
        {
            "jpName": "シュヴァルグラン",
            "enName": "Cheval Grand",
            "jpText": "……はぁっ……はぁっ……はぁっ……！\n僕…………やったんだ……！",
            "enText": "…… Haa…… Haa…… Haa…… ! \nI…… did it…… !",
            "nextBlock": 15,
            "origClipLength": 410,
            "voiceIdx": 13,
            "animData": [
                {
                    "origLen": 17,
                    "pathId": -7878137653060631435
                },
                {
                    "origLen": 60,
                    "pathId": -2995377678216922948
                },
                {
                    "origLen": 111,
                    "pathId": 9158578390495408255
                }
