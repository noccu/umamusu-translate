            "pathId": -4999033449879664285,
            "blockIdx": 10
        },
        {
            "jpName": "エアグルーヴ",
            "enName": "Air Groove",
            "jpText": "！　姉君って――",
            "enText": "Oh, my God! My sister…",
            "nextBlock": 12,
            "origClipLength": 55,
            "animData": [
                {
                    "origLen": 66,
                    "pathId": 7062774018344234920
                },
                {
                    "origLen": 66,
