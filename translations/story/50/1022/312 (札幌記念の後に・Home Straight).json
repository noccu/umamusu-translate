        },
        {
            "jpName": "SP隊長",
            "enName": "SP Commander",
            "jpText": "殿下！\n姉君からお電話です。",
            "enText": "Your Highness! Your sister is calling.",
            "nextBlock": 11,
            "origClipLength": 63,
            "animData": [
                {
                    "origLen": 49,
