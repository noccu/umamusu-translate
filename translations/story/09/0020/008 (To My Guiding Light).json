            "pathId": 7523784309656384606,
            "blockIdx": 25
        },
        {
            "jpName": "サクラローレル",
            "enName": "Sakura Laurel",
            "jpText": "ねぇ、ブライアンちゃん。",
            "enText": "Isn't it, Brian?",
            "nextBlock": 27,
            "origClipLength": 68,
            "animData": [
                {
                    "origLen": 89,
                    "pathId": 8523006163492581865
                },
