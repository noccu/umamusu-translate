            "pathId": 8438849880680881094,
            "blockIdx": 6
        },
        {
            "jpName": "セイウンスカイ",
            "enName": "Seiun Sky",
            "jpText": "…………。",
            "enText": "……",
            "nextBlock": 8,
            "origClipLength": 75,
            "animData": [
                {
                    "origLen": 90,
