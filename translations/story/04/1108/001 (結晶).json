            "blockIdx": 1
        },
        {
            "jpName": "ドゥラメンテ",
            "enName": "Duramente",
            "jpText": "……“最強”。",
            "enText": "……\"The Strongest\"",
            "nextBlock": 3,
            "origClipLength": 75,
            "voiceIdx": 1,
            "animData": [
                {
                    "origLen": 86,
                    "pathId": -926532277003142139
