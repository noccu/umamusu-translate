            "blockIdx": 33
        },
        {
            "jpName": "ウマ娘",
            "enName": "Horsegirl",
            "jpText": "きゃあっ！？",
            "enText": "Kyaa!?",
            "nextBlock": 35,
            "origClipLength": 28,
            "voiceIdx": 23,
            "pathId": -6113483614714028172,
            "blockIdx": 34
        },
        {
            "jpName": "イベント実況",
