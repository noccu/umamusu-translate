            "pathId": 6881073639545456543,
            "blockIdx": 2
        },
        {
            "jpName": "ゴールドシチー",
            "enName": "Gold City",
            "jpText": "うん。",
            "enText": "Yeah.",
            "nextBlock": 4,
            "choices": [
                {
                    "jpText": "……",
                    "enText": "…",
                    "nextBlock": 4
