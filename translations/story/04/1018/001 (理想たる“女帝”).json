            "pathId": 4027720433904507921,
            "blockIdx": 15
        },
        {
            "jpName": "サイレンススズカ",
            "enName": "Silence Suzuka",
            "jpText": "はっ、はっ、はっ……！！",
            "enText": "Ha, ha, ha…!",
            "nextBlock": 17,
            "origClipLength": 82,
            "animData": [
                {
                    "origLen": 115,
                    "pathId": 8713806496889080836
                },
                {
                    "origLen": 112,
                    "pathId": 5754945607046445950
                }
            ],
            "pathId": 2241264582458154141,
            "blockIdx": 16
        },
        {
            "jpName": "フジキセキ",
            "enName": "Fuji Kiseki",
            "jpText": "おおっ……！\r\nスズカの逃げ、いつ見ても見事だね。",
            "enText": "Ohh…! \nSuzuka's runaway never fails to amaze me.",
            "nextBlock": 18,
