            "blockIdx": 23
        },
        {
            "jpName": "ドゥラメンテ",
            "enName": "Duramente",
            "jpText": "………………。",
            "enText": "………………",
            "nextBlock": 25,
            "origClipLength": 30,
            "voiceIdx": 18,
            "animData": [
                {
                    "origLen": 69,
                    "pathId": -5594129545006375428
                },
                {
                    "origLen": 68,
                    "pathId": 6242580862305965839
