            "nextBlock": 3,
            "origClipLength": 85,
            "pathId": 6677889993424534902,
            "blockIdx": 2
        },
        {
            "jpName": "キタサンブラック",
            "enName": "Kitasan Black",
            "jpText": "うわっ！？\r\nあ、すみません！　大丈夫ですか！？",
            "enText": "Whoa! Oh, I'm sorry! Are you okay?",
            "nextBlock": 4,
            "origClipLength": 76,
            "choices": [
                {
                    "jpText": "大丈夫。気合い入ってるな",
                    "enText": "I'm fine. You're fired up.",
                    "nextBlock": 4
                },
                {
                    "jpText": "大丈夫。気合い入ってるね",
                    "enText": "I'm fine. You're fired up.",
