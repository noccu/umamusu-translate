            "pathId": 1111948049819388295,
            "blockIdx": 3
        },
        {
            "jpName": "シンボリクリスエス",
            "enName": "Symboli Kris S",
            "jpText": "いや、―――…………。",
            "enText": "No―――…………",
            "nextBlock": 5,
            "origClipLength": 104,
            "voiceIdx": 2,
            "choices": [
                {
                    "jpText": "どうかしたのか？",
                    "enText": "Is something wrong?",
                    "nextBlock": 5
                },
                {
                    "jpText": "どうかしたの？",
                    "enText": "Is something wrong?",
                    "nextBlock": 5
