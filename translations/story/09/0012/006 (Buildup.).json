            "pathId": 3864897770793483008,
            "blockIdx": 7
        },
        {
            "jpName": "セイウンスカイ",
            "enName": "Seiun Sky",
            "jpText": "…………。\nはぁ～。",
            "enText": "…… Faah～",
            "nextBlock": 9,
            "origClipLength": 215,
            "animData": [
                {
                    "origLen": 90,
                    "pathId": 6168462307222843815
