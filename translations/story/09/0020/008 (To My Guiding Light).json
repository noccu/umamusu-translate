            "pathId": -4398244898327817061,
            "blockIdx": 24
        },
        {
            "jpName": "サクラローレル",
            "enName": "Sakura Laurel",
            "jpText": "……キレイ。",
            "enText": "… It's so pretty.",
            "nextBlock": 26,
            "origClipLength": 81,
            "animData": [
                {
                    "origLen": 24,
                    "pathId": -3612693365158917337
