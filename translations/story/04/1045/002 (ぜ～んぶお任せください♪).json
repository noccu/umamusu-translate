                },
                {
                    "origLen": 116,
                    "pathId": 599412201385794810
                }
            ],
            "pathId": 6895869935609468840,
            "blockIdx": 32
        },
        {
            "jpName": "スーパークリーク",
            "enName": "Super Creek",
            "jpText": "……って、あら？",
            "enText": "Ah- huh?",
            "nextBlock": 34,
            "origClipLength": 55,
            "animData": [
                {
                    "origLen": 105,
                    "pathId": -352658235207455361
                },
                {
                    "origLen": 41,
                    "pathId": 7518246939530263461
                }
            ],
            "pathId": -1382463634653512501,
            "blockIdx": 33
        },
        {
            "jpName": "モノローグ",
