        {
            "jpName": "ゴールドシチー",
            "enName": "Gold City",
            "jpText": "……ふっ。",
            "enText": "… phew.",
            "nextBlock": 4,
            "pathId": -4521613290124144840,
            "blockIdx": 3
        },
        {
            "jpName": "ゴールドシチー",
            "enName": "Gold City",
            "jpText": "なんか、アンタが自信満々だと、\nうさんくさいんだけど。",
            "enText": "I feel like you're being too confident.",
            "nextBlock": 5,
            "choices": [
                {
                    "jpText": "ええっ！？",
                    "enText": "Eh?",
                    "nextBlock": 5
                }
