            "blockIdx": 11
        },
        {
            "jpName": "タマモクロス",
            "enName": "Tamamo Cross",
            "jpText": "……………………。",
            "enText": "…………………",
            "nextBlock": 13,
            "origClipLength": 56,
            "animData": [
                {
                    "origLen": 89,
                    "pathId": -3118337865456356251
