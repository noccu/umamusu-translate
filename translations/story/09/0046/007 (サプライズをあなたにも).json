        {
            "jpName": "サトノクラウン",
            "enName": "Satono Crown",
            "jpText": "撮影スタッフさんたち！？",
            "enText": "All of the photoshoot staff!?",
            "nextBlock": 11,
            "origClipLength": 72,
            "voiceIdx": 9,
            "animData": [
