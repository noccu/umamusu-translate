            "nextBlock": 2,
            "origClipLength": 216,
            "voiceIdx": 0,
            "choices": [
                {
                    "jpText": "――シオン、まだ練習してたのか？",
                    "enText": "Shion, you were still practicing?",
                    "nextBlock": 2
