                },
                {
                    "origLen": 163,
                    "pathId": 2094212308693842220
                }
            ],
            "pathId": -8930402719602673922,
            "blockIdx": 3
        },
        {
            "jpName": "カワカミプリンセス",
            "enName": "",
            "jpText": "いっきますわよ～～っ！！",
            "enText": "I'm coming!",
            "nextBlock": 5,
            "origClipLength": 75,
            "animData": [
                {
                    "origLen": 50,
                    "pathId": -8090062762001116824
                },
                {
                    "origLen": 20,
                    "pathId": -194175634124473246
