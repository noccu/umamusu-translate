            "animData": [
                {
                    "origLen": 242,
                    "pathId": -8509864432487661574
                }
            ],
            "pathId": 8916801559177227609,
            "blockIdx": 34
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "なるほどなるほど、そういうお考えでしたか。",
