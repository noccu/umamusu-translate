        {
            "jpName": "フジキセキ",
            "enName": "Fuji Kiseki",
            "jpText": "ん？",
            "enText": "Hm?",
            "nextBlock": 20,
            "origClipLength": 28,
            "animData": [
                {
                    "origLen": 60,
                    "pathId": 765635412493409746
                },
                {
                    "origLen": 144,
