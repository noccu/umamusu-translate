            "blockIdx": 22
        },
        {
            "jpName": "ダイタクヘリオス",
            "enName": "Daitaku Helios",
            "jpText": "どこ行くん？？　うぉーい！",
            "enText": "Where are you going?? Heeeey!",
            "nextBlock": 24,
            "origClipLength": 94,
            "voiceIdx": 18,
            "choices": [
                {
                    "jpText": "俺に任せて休んでて！",
                    "enText": "Leave it to me and rest!",
                    "nextBlock": 24
                },
                {
                    "jpText": "私に任せて休んでて！",
                    "enText": "Leave it to me and rest!",
                    "nextBlock": 24
                }
