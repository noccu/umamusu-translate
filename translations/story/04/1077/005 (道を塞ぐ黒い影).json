        {
            "jpName": "ナリタトップロード",
            "enName": "Narita Top Road",
            "jpText": "っっっ！\n……～～～～～～～。",
            "enText": "Hiiii!! \n……~~~~~~",
            "nextBlock": 18,
            "origClipLength": 129,
            "voiceIdx": 10,
            "choices": [
                {
                    "jpText": "もしかして……カラスが苦手なのか？",
                    "enText": "Are you……bad with crows?",
                    "nextBlock": 18
                },
                {
                    "jpText": "もしかして……カラスが苦手なの？",
                    "enText": "Are you……bad with crows?",
                    "nextBlock": 18
                }
            ],
            "animData": [
