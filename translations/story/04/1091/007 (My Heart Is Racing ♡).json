            "origClipLength": 102,
            "voiceIdx": -1,
            "pathId": -1657037826043322908,
            "blockIdx": 1
        },
        {
            "jpName": "ヴィブロス",
            "enName": "Vivlos",
            "jpText": "トレっち～！　こっちこっち♪",
            "enText": "Tore-cchi~! Over here, over here♪",
