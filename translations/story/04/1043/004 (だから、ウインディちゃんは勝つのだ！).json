            "nextBlock": 22,
            "origClipLength": 96,
            "voiceIdx": 14,
            "choices": [
                {
                    "jpText": "（よし、冷静だ……！）",
                    "enText": "(Alright, she's staying calm……!)",
                    "nextBlock": 22
                }
