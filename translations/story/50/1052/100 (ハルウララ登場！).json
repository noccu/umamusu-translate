            "blockIdx": 8
        },
        {
            "jpName": "ハルウララ",
            "enName": "Haru Urara",
            "jpText": "ああ～～～～っ！！",
            "enText": "Ohhhhh!!",
            "nextBlock": 10,
            "origClipLength": 50,
            "choices": [
                {
                    "jpText": "どうした！？",
                    "enText": "What happened?!",
                    "nextBlock": 10
                },
                {
                    "jpText": "どうしたの！？",
                    "enText": "What happened?!",
                    "nextBlock": 10
                }
            ],
            "animData": [
                {
                    "origLen": 78,
                    "pathId": -6964724640713941571
                },
                {
                    "origLen": 39,
                    "pathId": 3714822907168182951
