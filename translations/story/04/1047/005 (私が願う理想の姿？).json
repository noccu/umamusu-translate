            "pathId": 3010377430085696188,
            "blockIdx": 13
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "『主役は君だ、ロブロイ！！』",
            "enText": "『You're the main character, Rob Roy!!』",
            "nextBlock": 15,
            "origClipLength": 63,
            "voiceIdx": -1,
            "animData": [
