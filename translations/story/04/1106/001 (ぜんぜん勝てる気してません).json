    "title": "ぜんぜん勝てる気してません",
    "enTitle": "",
    "text": [
        {
            "jpName": "シンボリクリスエス",
            "enName": "Symboli Kris S",
            "jpText": "……――",
            "enText": "……――",
            "nextBlock": 2,
            "origClipLength": 26,
            "voiceIdx": 0,
            "animData": [
                {
                    "origLen": 73,
                    "pathId": -1525485168771036997
