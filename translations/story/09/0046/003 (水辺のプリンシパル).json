            "blockIdx": 1
        },
        {
            "jpName": "ウインバリアシオン",
            "enName": "Win Variation",
            "jpText": "（主役を……全うしてみせる……！）",
            "enText": "(I'll fulfill… my role of the principal…!)",
            "nextBlock": 3,
            "origClipLength": 109,
            "voiceIdx": 1,
            "animData": [
                {
                    "origLen": 16,
                    "pathId": 8667835951989994815
