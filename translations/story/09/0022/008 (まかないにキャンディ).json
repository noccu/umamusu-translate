            "voiceIdx": 22,
            "pathId": -8472476492676160779,
            "blockIdx": 24
        },
        {
            "jpName": "ビコーペガサス",
            "enName": "Biko Pegasus",
            "jpText": "ようこそ！",
            "enText": "Welcome!",
            "nextBlock": 26,
            "origClipLength": 40,
            "voiceIdx": 23,
