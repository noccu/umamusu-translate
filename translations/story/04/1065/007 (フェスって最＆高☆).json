            "nextBlock": 27,
            "origClipLength": 81,
            "voiceIdx": 21,
            "pathId": 7800914733963134719,
            "blockIdx": 26
        },
        {
            "jpName": "ベテラントレーナー",
            "enName": "Veteran Trainer",
