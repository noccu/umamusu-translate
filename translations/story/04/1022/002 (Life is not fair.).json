            "nextBlock": 3,
            "origClipLength": 254,
            "choices": [
                {
                    "jpText": "君は……ファインモーション殿下！？",
                    "enText": "You're… Your Highness, Fine Motion!?",
                    "nextBlock": 3
                },
                {
                    "jpText": "貴方は……ファインモーション殿下！？",
                    "enText": "You're… Your Highness, Fine Motion!?",
                    "nextBlock": 3
