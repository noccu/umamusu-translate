        },
        {
            "jpName": "ナリタブライアン",
            "enName": "Narita Brian",
            "jpText": "……むぅ。",
            "enText": "…… mmm.",
            "nextBlock": 32,
            "origClipLength": 51,
            "animData": [
                {
                    "origLen": 9,
                    "pathId": -7668350525770223901
