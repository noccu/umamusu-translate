            "nextBlock": 2,
            "origClipLength": 228,
            "voiceIdx": 0,
            "choices": [
                {
                    "jpText": "ああ、お疲れ様！",
                    "enText": "Yeah, good work!",
                    "nextBlock": 2
                },
                {
                    "jpText": "うん、お疲れ様！",
                    "enText": "Yeah, good work!",
                    "nextBlock": 2
                }
            ],
